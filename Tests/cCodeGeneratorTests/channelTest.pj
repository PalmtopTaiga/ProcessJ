/**
 * Not so simple program, creates channel and has
 * source talk to sink.
*/
import std.io;

//Source that writes 2 forever.
proc void source(chan<int>.write chanW) {
  int x = 2;

  while (true)
    chanW.write(x);

  return;
}

proc void procMain() {
  chan<int> intChan;
  source(intChan);
  return;
}

